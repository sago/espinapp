<div class="vertical-center">
  <div class="signInBox">
    <div class="header">
      <h4>Agregar Usuario</h4>
    </div>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <div class="field">
        <%= f.label "Nombre" %><br />
        <%= f.email_field :first_name, autofocus: true, class: "form-control" %>
      </div>
      <br>
      <div class="field">
        <%= f.label "Apellido" %><br />
        <%= f.email_field :last_name, autofocus: true, class: "form-control" %>
      </div>
      <br>
      <div class="field">
        <%= f.label "Correo" %><br />
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control" %>
      </div>
      <br>
      <div class="field">
        <%= f.label "ContraseÃ±a" %><br />
        <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
      </div>
      <br>
      <div class="field">
        <%= f.label "Rol:" %><br />
        <%= f.select :role, ["admin", "repartidor"], { include_blank: "Select..." }, { class: "form-control" } %>
      </div>
      <br>

    <% end %>
  </div>
</div>
